{
  "editor.rulers": [100],
  "files.associations": {
    "*.yml": "yaml",
    "*.yaml": "yaml"
  },
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": ".github/workflows/*.yml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.rulers": [80]
  }
}
